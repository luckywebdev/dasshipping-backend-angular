{
  "name": "project-name",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=dev ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=dev nodemon",
    "start:debug": "cross-env NODE_ENV=dev nodemon --config nodemon-debug.json",
    "start:staging": "cross-env NODE_ENV=staging node dist/main.js",
    "start:production": "cross-env NODE_ENV=production node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "typeorm": "cross-env NODE_ENV=local ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm migration:create -d src/migrations",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:generate": "yarn typeorm migration:generate",
    "fixtures:init": "cross-env NODE_ENV=dev ts-node src/scripts/fixtures-init.ts",
    "fixtures:clear": "cross-env NODE_ENV=dev ts-node src/scripts/fixtures-clear.ts"
  },
  "dependencies": {
    "@nestjs/common": "6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.2",
    "@nestjs/platform-socket.io": "^6.3.0",
    "@nestjs/swagger": "^3.0.1",
    "@nestjs/typeorm": "^6.0.0",
    "@nestjs/websockets": "^6.3.0",
    "@types/ejs": "^2.6.3",
    "@types/html-pdf": "^2.1.2",
    "@types/joi": "^14.3.2",
    "aws-sdk": "^2.427.0",
    "axios": "^0.19.0",
    "braintree": "^2.19.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "cross-env": "^5.2.0",
    "crypto-js": "^3.1.9-1",
    "csvjson": "^5.1.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^7.0.0",
    "ejs": "^2.6.2",
    "html-pdf": "^2.2.0",
    "joi": "^14.3.1",
    "logstash-client": "^1.1.1",
    "mailgun-js": "^0.22.0",
    "mock-express-response": "^0.2.2",
    "moment": "^2.24.0",
    "multer-s3": "^2.9.0",
    "nest-emitter": "^1.1.0",
    "nest-schedule": "^0.6.2",
    "nestjs-apm": "^1.0.9",
    "node-deeplink": "git+https://github.com/georgel2489/node-deeplink.git",
    "onesignal-api-client-core": "^1.1.12",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^7.9.0",
    "phantomjs-bin": "^2.1.1",
    "ramda": "^0.26.1",
    "randomstring": "^1.1.5",
    "recaptcha2": "^1.3.3",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.0",
    "rxjs": "^6.3.3",
    "sha1": "^1.1.1",
    "shortid": "^2.2.15",
    "tesseract.js": "^1.0.19",
    "tesseract.ts": "^1.1.2",
    "typeorm": "^0.2.15",
    "winston": "^3.2.1",
    "zip-folder": "^1.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/ramda": "^0.26.8",
    "@types/socket.io": "^2.1.2",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}